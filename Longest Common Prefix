#include <bits/stdc++.h>
#include <stdio.h>

using namespace std;

int main()
{
    unordered_map <int,int> m;
    string s[3] = {"flower","flow","flight"};
    
    //cout << s.length();
    

    for(int i=0;i<3;i++)
    {
        for(int j=0;j<s[i].length();j++)
        {
            m[s[i][j]]++;
        }
    }
    
    for(int i=0;i<s[0].length();i++)
    {
        if(m[s[0][i]]==3)
        {
        cout << s[0][i] << "";
        }
    }
    
    

    return 0;
}

